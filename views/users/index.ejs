<!DOCTYPE html>
<html lang="en" ng-app="myModule">
<head>
    <% include ../partials/head %>
</head>
<body class="container" ng-controller="mainCtrl">
    <header>
        <% include ../partials/header %>
    </header>


    <main>

        <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.3.10/angular.min.js"></script>
        <script src="javascripts/core.js"></script>

        <div class="jumbotron">
            <h1>Users</h1>
            <p>Let me introduce about my users</p>
        </div>

        <table class="table">
          <tr>
            <th>ID</th><th>firstName</th><th>lastName</th><th>email</th><th>device</th><th>ua</th><th>signupPagePattern</th>
          </tr>
          <tr ng-repeat="user in users">
            <td>{{user._id}}</td>
            <td>{{user.firstName}}</td>
            <td>{{user.lastName}}</td>
            <td>{{user.email}}</td>
            <td>{{user.device}}</td>
            <td>{{user.userAgent}}</td>
            <td>{{user.signupPagePattern}}</td>
          </tr>
        </table>


    </main>

    <footer>
        <% include ../partials/footer %>
    </footer>
    
</body>
</html>





